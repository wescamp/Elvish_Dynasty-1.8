#: Elvish_Dynasty/scenarios/5aED.cfg:174 Elvish_Dynasty/scenarios/5aED.cfg:221
#: Elvish_Dynasty/scenarios/5aED.cfg:254 Elvish_Dynasty/scenarios/5aED.cfg:287
#: Elvish_Dynasty/scenarios/5aED.cfg:320
msgid "Diggers"
msgstr ""

#. [side]: id=Dugard, type=Dwarvish Stalwart
#: Elvish_Dynasty/scenarios/5aED.cfg:70
msgid "Dugard Goldheim"
msgstr ""

#. [side]: id=Jerrol, type=Dwarvish Pathfinder
#: Elvish_Dynasty/scenarios/5aED.cfg:128
