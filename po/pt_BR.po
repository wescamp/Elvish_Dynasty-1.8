msgid "General Brosan"
msgstr ""

#. [unit]: type=General, id=Alvi
#: Elvish_Dynasty/scenarios/EDTier2Finale.cfg:299
msgid "General Alvi"
msgstr ""

#. [unit]: type=General, id=Marille
#: Elvish_Dynasty/scenarios/EDTier2Finale.cfg:314
msgid "General Marille"
msgstr ""

#. [unit]: type=General, id=Evanson
#: Elvish_Dynasty/scenarios/EDTier2Finale.cfg:329
msgid "General Evanson"
msgstr ""

